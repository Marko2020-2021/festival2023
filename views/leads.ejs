    <script>      
      var socket = io();
    </script>
    <script>      
      ///socket.emit('chat message', 'al inicio1');  
      let r=0
      
      socket.on('chat message', (msg)=> {
        {
          const ff = 'TT'+JSON.stringify(msg)+'GG'
   console.log('delphi',ff)
          var V1 = msg.split(':')[1].split(';')[0];
