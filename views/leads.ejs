          var N6 = msg.split(':')[16].split(';')[0]; 
          var N7 = msg.split(':')[17].split(';')[0]; 
          var N8 = msg.split(':')[18].split(';')[0]; 
          var N9 = msg.split(':')[19].split(';')[0]; 
          var N10 = msg.split(':')[20].split(';')[0]; 
          
          document.getElementById("demo1").innerHTML = N1;
          document.getElementById("demo2").innerHTML = V1;
          document.getElementById("demo3").innerHTML = N2;
          document.getElementById("demo4").innerHTML = V2;
          document.getElementById("demo5").innerHTML = N3;
          document.getElementById("demo6").innerHTML = V3;
          document.getElementById("demo7").innerHTML = N4;
          document.getElementById("demo8").innerHTML = V4;
          document.getElementById("demo9").innerHTML = N5;
          document.getElementById("demo10").innerHTML = V5;
          document.getElementById("demo11").innerHTML = N6;
          document.getElementById("demo12").innerHTML = V6;
          document.getElementById("demo13").innerHTML = N7;
          document.getElementById("demo14").innerHTML = V7;
          document.getElementById("demo15").innerHTML = N8;
          document.getElementById("demo16").innerHTML = V8;
          document.getElementById("demo17").innerHTML = N9;
          document.getElementById("demo18").innerHTML = V9;
          document.getElementById("demo19").innerHTML = N10;
          document.getElementById("demo20").innerHTML = V10;
        }
          r=r+1
      });
      
    </script>
